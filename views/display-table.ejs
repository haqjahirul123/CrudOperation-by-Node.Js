<%- include('header'); -%>


<% if(users.length>0) { %>

<table border="1">
        <tr>
                <th>Index</th>
                <th>Model</th>
                <th>ManufractureYear</th>
                <th>Price</th>
                <th>Sail</th>
                <th>Motor</th>
        </tr>
        <% for(var i=0; i<users.length;i++) { %>
                <tr>
                     
                  <td><%= i %></td>
                  <td><%= users[i].boat_name %></td>
                  <td><%= users[i].boat_manufractureYear %></td>
                  <td><%= users[i].boat_price %></td>
                  <td><%= users[i].boat_sail %></td>
                  <td><%= users[i].boat_motor %></td>
                        <td>
                          <a href='/edit/<%= users[i]._id %>'>Edit</a> | 
                          <a href='/delete/<%= users[i]._id %>'>Delete</a> |
                          <a href='/show/<%= users[i]._id %>'>Show</a>
                
                        </td>
                </tr>
              <% } %>
        
</table>

<% } else { %>
        No Record Found
      <% } %>