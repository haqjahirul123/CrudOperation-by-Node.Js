 

<%- include('header'); -%>

<form method="post">

    <table>

        <tr>
            <td>Model : </td>
            <td>
                <input type="text" placeholder="Enter Model" name="boat_name" required> </td>
        </tr>


        <tr>
            <td> ManufractureYear :</td>
            <td>
                <input type="text" placeholder="Enter ManufractureYear" name="boat_manufractureYear" required>
